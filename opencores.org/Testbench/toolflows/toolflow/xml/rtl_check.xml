  <spirit:vendorExtensions>
    <socgen:envIdentifier>:*Lint:*</socgen:envIdentifier>
  </spirit:vendorExtensions>
  <spirit:generatorExe>./tools/sys/gen_child_filelist</spirit:generatorExe>
